    "n = 4\n",
    "p = []\n",
    "comp = 1\n",
    "sum_val = 0\n",
    "\n",
    "for i in range(n):\n",
    "    comp *= (10 - i)/((i+1) / 2 + 5)\n",
    "    sum_val += comp\n",
    "\n",
    "p_0 = 1/(1+sum_val)\n",
    "\n",
    "for i in range(n):\n",
    "    comp = 1\n",
    "    comp = (10 - i)/((i+1) / 2 + 5)\n",
    "    p.append(p_0*comp)\n",
    "    \n",
    "print(p,'\\n', sum(p))"
